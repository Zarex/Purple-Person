<idea-plugin>
    <id>se.piksel.intellij.themes.prpl</id>
    <name>Prpl Theme</name>
    <version>0.6.0</version>
    <vendor email="nils@piksel.se" url="https://piksel.se">piksel bitworks</vendor>
    <description><![CDATA[
      <p>Vibrant dark purple theme for IntelliJ-based editors</p>
      <p>Certainly monokaish, but mostly purple</p>
    ]]></description>

    <change-notes><![CDATA[
      <ul>
        <li>Usability and contrast fixes</li>
        <li>Remove transparency from unintended places</li>
      </ul>
    ]]>
    </change-notes>

    <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="192.0"/>

    <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
        <themeProvider id="1689629a-4749-46da-b8b2-497dfc97fefb" path="/META-INF/Prpl.theme.json"/>
        <!--<themeMetadataProvider path="/META-INF/Prpl.themeMetadata.json"/>-->
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>

</idea-plugin>